{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": ["off"],
    "no-console": ["off"],
    "prettier/prettier": ["error", { "singleQuote": true, "parser": "flow" }],
    "import/no-unresolved": [2, { "ignore": [".img$"] }],
    "no-param-reassign": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off"
  }
}
